<!DOCTYPE html>
<html>
<head>
	<title><%= @title%></title>
</head>
<body>
	<%= erb(:'partials/navigation', :layout => false) %>
	<h1>Welcome to the home page</h1>


	<% @Posts.each do |post| %>
	<div>
	       <h3><a href="/<%= post[:id] %>"><%= post[:title] %></a></h3>
	       <p><%= post[:body]%></p>
	</div>
	<% end %>
</body>
</html>